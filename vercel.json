{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/users(.*)",
        "dest": "./routers/usersRouter.js"
      },
      {
        "src": "/lighting(.*)",
        "dest": "./routers/lightningRouter.js"
      }
    ]
  }
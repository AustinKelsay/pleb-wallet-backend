{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/users",
        "destination": "/routers/usersRouter.js"
      },
      {
        "source": "/lighting/*",
        "destination": "/routers/lightningRouter.js"
      },
      {
        "source": "/",
        "destination": "/index.js"
      }
    ]
  }
  